#include <iostream>
#include <string>
#include <vector> 
#include <algorithm> 

std::string toBinary(char m)
{
    int n = static_cast <int>(m);
    std::string r;
    while (n != 0)
    {
        r += ( n % 2 == 0 ? "0" : "1" );
        n /= 2;
    }
    return r;
}

std::vector<std::string> to_vec(std::string str)
{
    std::vector<std::string> out;
    out.resize(str.size());
    long long i = 0;
    for (char elem : str)
    {
        out[i] = toBinary(elem);
        std::cout << out[i] << std::endl;
        ++i;
    }
    return out;
}

int main()
{
    char a;
    while (true)
    {
        std::cin >> a;
        std::cout << toBinary(a) << std::endl;
    }
}
